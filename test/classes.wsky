var testing;
var equal;


var testClass = {
    class Duck ();

    // equal(Class, Duck.class);
    equal('<Class Duck>', Duck.toString);
    // equal(Duck, Duck().class);
    equal('<Duck>', Duck().toString);

    class A (
        init {}
    );
    // equal(A, A().class);
    equal('<A>', A().toString);
};


export runTests = {testing_:
    testing = testing_;
    equal = testing_.equal;

    testClass();
};
